{
  "functions": {
    "api/market-data.js": { "maxDuration": 10 },
    "api/claude.js": { "maxDuration": 30 },
    "api/auth.js": { "maxDuration": 10 },
    "api/email-auth.js": { "maxDuration": 10 },
    "api/verify-pro.js": { "maxDuration": 10 },
    "api/validate-token.js": { "maxDuration": 10 },
    "api/portfolios.js": { "maxDuration": 10 },
    "api/stripe-webhook.js": { "maxDuration": 10 },
    "api/db-setup.js": { "maxDuration": 30 }
  },
  "headers": [
    {
      "source": "/data.js",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400, stale-while-revalidate=604800" }
      ]
    },
    {
      "source": "/app.js",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400, stale-while-revalidate=604800" }
      ]
    },
    {
      "source": "/patches.js",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400, stale-while-revalidate=604800" }
      ]
    },
    {
      "source": "/styles.css",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400, stale-while-revalidate=604800" }
      ]
    },
    {
      "source": "/api/market-data",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=3600, stale-while-revalidate=600"
        }
      ]
    },
    {
      "source": "/api/verify-pro",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=86400, stale-while-revalidate=3600"
        }
      ]
    },
    {
      "source": "/api/validate-token",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=86400, stale-while-revalidate=3600"
        }
      ]
    },
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
